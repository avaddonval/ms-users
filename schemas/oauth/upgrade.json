{
  "$id": "oauth.upgrade",
  "type": "object",
  "required": ["provider", "token"],
  "properties": {
    "provider": {
      "type": "string",
      "enum": ["facebook", "apple"]
    },
    "token": {
      "type": "string",
      "maxLength": 1024,
      "minLength": 1
    },
    "jwt": {
      "type": "string",
      "minLength": 1,
      "maxLength": 1024
    }
  }
}
